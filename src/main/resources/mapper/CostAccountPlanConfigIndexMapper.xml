<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hscloud.hs.cost.account.mapper.CostAccountPlanConfigIndexMapper">

    <select id="selectIndexByConfigId"
            resultType="com.hscloud.hs.cost.account.model.entity.CostAccountPlanConfigIndex">
        SELECT
            *
        FROM
            cost_account_plan_config_index
        WHERE
            plan_config_id = #{configId}
    </select>
</mapper>
